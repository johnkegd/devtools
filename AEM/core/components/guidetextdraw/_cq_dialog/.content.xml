<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title=""
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[af.authoring.dialog]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <accordion
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <placeholder
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <value
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"
                                sling:orderBefore="fieldLayout"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                disabled="{Boolean}false"
                                fieldLabel="Default Value"
                                name="./_value"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <hideTitle
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <required
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <enabled
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <mandatoryMessage
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <dorBindref
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <guideGlobalLazyField
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <validateExpMessage
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <fieldLayout
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    guideDataModel="basic"
                                    isAfField="true"
                                    layoutType="fd/af/layouts/field"
                                    type="layout"/>
                            </fieldLayout>
                        </items>
                    </basic>
                    <help
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <patterns
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <accessibility
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                </items>
            </accordion>
        </items>
    </content>
</jcr:root>
