<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title=""
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[af.authoring.dialog]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <accordion
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldLabel="Name"
                                name="./name"
                                regexText="Element name should only contain characters or numbers"
                                required="true"
                                xtype="textfield">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    node-name-validation=""/>
                            </name>
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title"
                                name="./jcr:title"
                                xtype="textfield"/>
                            <description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldLabel="Description"
                                name="./jcr:description"
                                xtype="textfield"/>
                            <fragRef
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="fd/af/granite/components/formpicker"
                                fieldDescription="Selected panel will replace the content of the panel."
                                fieldLabel="Form or Fragment"
                                filter="afFragment"
                                name="./fragRef"
                                rootPath="/content/dam/formsanddocuments"/>
                            <repeatSettings
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Repeat Settings"
                                sling:resourceType="fd/af/authoring/components/granite/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <minOccur
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue=""
                                        fieldLabel="Minimum"
                                        min="0"
                                        name="./minOccur"/>
                                    <maxOccur
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue=""
                                        fieldLabel="Maximum"
                                        min="-1"
                                        name="./maxOccur">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            node-maxoccurance-validation=""/>
                                    </maxOccur>
                                </items>
                            </repeatSettings>
                            <bindref
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/bindref"/>
                            <panelitemslayout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                disabled="false"
                                fieldLabel="Layout"
                                name="./layout/sling:resourceType"
                                renderReadOnly="false">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    guideDataModel="basic"
                                    layoutType="fd/af/layouts/panel"
                                    type="layout"/>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.layoutOnChange('change');"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.layoutOnChange();"/>
                            </panelitemslayout>
                            <nonNavigable
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./layout/nonNavigable"/>
                            <guideNavigatorTab
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./layout/guideNavigatorTab"/>
                            <enableLayoutOptimization
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./layout/enableLayoutOptimization"/>
                            <completionExpReq
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./completionExpReq"/>
                            <completionExpReq-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./completionExpReq@TypeHint"
                                value="Boolean"/>
                            <toolbarPosition
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                disabled="false"
                                fieldLabel="Position of Toolbar"
                                name="./layout/toolbarPosition"
                                renderReadOnly="false">
                                <items jcr:primaryType="nt:unstructured">
                                    <v1
                                        jcr:primaryType="nt:unstructured"
                                        text="Bottom"
                                        value="Bottom"/>
                                    <v2
                                        jcr:primaryType="nt:unstructured"
                                        text="Top"
                                        value="Top"/>
                                </items>
                            </toolbarPosition>
                            <colspan
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete"
                                disabled="false"
                                fieldDescription="Enter number of columns the field should span. Supported only for Responsive layout."
                                fieldLabel="Colspan"
                                name="./colspan"
                                renderReadOnly="false">
                                <options
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete/list"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <empty
                                        jcr:primaryType="nt:unstructured"
                                        text=""
                                        value=""/>
                                    <v1
                                        jcr:primaryType="nt:unstructured"
                                        text="1"
                                        value="1"/>
                                    <v2
                                        jcr:primaryType="nt:unstructured"
                                        text="2"
                                        value="2"/>
                                    <v3
                                        jcr:primaryType="nt:unstructured"
                                        text="3"
                                        value="3"/>
                                    <v4
                                        jcr:primaryType="nt:unstructured"
                                        text="4"
                                        value="4"/>
                                    <v5
                                        jcr:primaryType="nt:unstructured"
                                        text="5"
                                        value="5"/>
                                    <v6
                                        jcr:primaryType="nt:unstructured"
                                        text="6"
                                        value="6"/>
                                </items>
                            </colspan>
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete"
                                fieldLabel="Number of Columns"
                                forceSelection="{Boolean}false"
                                name="./layout/columns"
                                value="1">
                                <options
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete/list"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <v1
                                        jcr:primaryType="nt:unstructured"
                                        text="1"
                                        value="1"/>
                                    <v2
                                        jcr:primaryType="nt:unstructured"
                                        text="2"
                                        value="2"/>
                                    <v3
                                        jcr:primaryType="nt:unstructured"
                                        text="3"
                                        value="3"/>
                                    <v4
                                        jcr:primaryType="nt:unstructured"
                                        text="4"
                                        value="4"/>
                                    <v5
                                        jcr:primaryType="nt:unstructured"
                                        text="5"
                                        value="5"/>
                                    <v6
                                        jcr:primaryType="nt:unstructured"
                                        text="6"
                                        value="6"/>
                                </items>
                            </columns>
                            <css
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="CSS Class"
                                name="./css"/>
                            <optimizeRenderPerformance
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldDescription="Associated fragment would be loaded lazily."
                                fieldLabel="Load fragment lazily"
                                name="./optimizeRenderPerformance"
                                text="Load fragment lazily"
                                value="true">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.showHideLazyLoadOption();"/>
                            </optimizeRenderPerformance>
                            <validateOnStepCompletion
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                class="completionScriptCheckBox"
                                fieldDescription="Enable to validate user inputs in the active panel and prevent navigation to the next panel if the validation fails."
                                name="./validateOnStepCompletion"
                                text="Validate active panel to proceed"
                                uncheckedValue="false"
                                value="true"/>
                            <validateOnStepCompletion-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./validateOnStepCompletion@TypeHint"
                                value="Boolean"/>
                            <visible
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/visible"
                                resourceType="granite/ui/components/coral/foundation/form/checkbox"/>
                            <visible-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/visible-typehint"
                                resourceType="granite/ui/components/coral/foundation/form/hidden"/>
                            <enabled
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/enabled"
                                resourceType="granite/ui/components/coral/foundation/form/checkbox"/>
                            <enabled-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/enabled-typehint"
                                resourceType="granite/ui/components/coral/foundation/form/hidden"/>
                        </items>
                        <parentConfig
                            jcr:primaryType="nt:unstructured"
                            active="{Boolean}true"/>
                    </basic>
                    <dor
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Document of Record"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <excludeFromDor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                checked="{Boolean}false"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/dor/items/excludeFromDor"
                                value="true"/>
                            <dorExcludeTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                checked="{Boolean}false"
                                name="./dorExcludeTitle"
                                text="Exclude title from Document of Record"
                                value="true"/>
                            <dorExcludeDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                checked="{Boolean}false"
                                name="./dorExcludeDescription"
                                text="Exclude description from Document of Record"
                                value="true"/>
                            <dorPagination
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Pagination"
                                sling:resourceType="fd/af/authoring/components/granite/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <breakBefore
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/granite/components/paginationpicker"
                                        emptyText="Following Previous"
                                        fieldDescription="Specifies where the panel is to be placed."
                                        fieldLabel="Place"
                                        name="./breakBeforeText"
                                        type="beforeBreakPaginationType"/>
                                    <breakAfter
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/granite/components/paginationpicker"
                                        emptyText="Continue Filling Parent"
                                        fieldDescription="Specifies which area to fill after the panel is placed."
                                        fieldLabel="After"
                                        name="./breakAfterText"
                                        type="afterBreakPaginationType"/>
                                    <overflow
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/granite/components/paginationpicker"
                                        emptyText="None"
                                        fieldDescription="Sets an overflow for a panel that spans pages."
                                        fieldLabel="Overflow"
                                        name="./overflowText"
                                        type="overflowPaginationType"/>
                                </items>
                            </dorPagination>
                            <dorLayoutType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                disabled="false"
                                fieldLabel="Layout for Document of Record"
                                name="./layout/dorLayoutType"
                                renderReadOnly="false">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.showHideDoRLayoutOptions();"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.showHideDoRLayoutOptions();"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        text="Column"
                                        value="columnar"/>
                                    <table
                                        jcr:primaryType="nt:unstructured"
                                        text="Table"
                                        value="tabular"/>
                                </items>
                            </dorLayoutType>
                            <dorNumCols
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                disabled="false"
                                fieldLabel="Number of Columns in Document of Record"
                                hidden="true"
                                name="./layout/dorNumCols"
                                renderReadOnly="false">
                                <items jcr:primaryType="nt:unstructured">
                                    <v1
                                        jcr:primaryType="nt:unstructured"
                                        text="1"
                                        value="1"/>
                                    <v2
                                        jcr:primaryType="nt:unstructured"
                                        text="2"
                                        value="2"/>
                                    <v3
                                        jcr:primaryType="nt:unstructured"
                                        text="3"
                                        value="3"/>
                                    <v4
                                        jcr:primaryType="nt:unstructured"
                                        text="4"
                                        value="4"/>
                                    <v5
                                        jcr:primaryType="nt:unstructured"
                                        text="5"
                                        value="5"/>
                                    <v6
                                        jcr:primaryType="nt:unstructured"
                                        text="6"
                                        value="6"/>
                                </items>
                            </dorNumCols>
                        </items>
                        <granite:rendercondition
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="fd/af/dor/rendercondition/tab"/>
                    </dor>
                    <help
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Help Content"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <longDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                disabled="{Boolean}false"
                                name="./longDescription"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <links jcr:primaryType="nt:unstructured">
                                        <linkDialogConfig
                                            jcr:primaryType="nt:unstructured"
                                            height="{Long}316">
                                            <linkAttributes jcr:primaryType="cq:WidgetCollection"/>
                                        </linkDialogConfig>
                                    </links>
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <keys
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <misctools
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <fullscreen
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,fullscreen#start]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                    ref="justify"/>
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists"/>
                                            </popovers>
                                        </inline>
                                        <fullscreen
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,-,links#modifylink,links#unlink,links#anchor,tracklinks#modifylinktracking,-,justify#justifyleft,justify#justifycenter,justify#justifyright,-,image#imageProps,-,lists#unordered,lists#ordered,lists#outdent,lists#indent,-,misctools#sourceedit,fullscreen#finish]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <styles
                                                    jcr:primaryType="nt:unstructured"
                                                    items="styles:getStyles:styles-pulldown"
                                                    ref="styles"/>
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                            </popovers>
                                        </fullscreen>
                                    </cui>
                                </uiSettings>
                            </longDescription>
                        </items>
                    </help>
                </items>
            </accordion>
        </items>
    </content>
</jcr:root>
