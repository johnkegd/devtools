<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    cq:dialogMode="floating"
    cq:disableTargeting="{Boolean}true"
    cq:layout="editbar"
    jcr:primaryType="cq:EditConfig">
    <cq:actionConfigs jcr:primaryType="nt:unstructured">
        <editToolbar
            jcr:primaryType="nt:unstructured"
            handler="guidelib.author.editConfigListeners.toolbar.editToolbar"
            text="Edit"/>
        <separator0
            jcr:primaryType="nt:unstructured"
            xtype="tbseparator"/>
        <deleteToolbar
            jcr:primaryType="nt:unstructured"
            handler="guidelib.author.editConfigListeners.toolbar.deleteToolbar"
            text="Delete"/>
        <separator1
            jcr:primaryType="nt:unstructured"
            xtype="tbseparator"/>
        <som
            jcr:primaryType="nt:unstructured"
            handler="guidelib.author.editConfigListeners.getSOM"
            text="View SOM Expression"/>
    </cq:actionConfigs>
</jcr:root>
