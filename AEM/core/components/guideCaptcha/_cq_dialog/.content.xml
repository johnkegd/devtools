<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title=""
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <accordion
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <validationMessage
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Validation Message"
                                sling:resourceType="fd/af/authoring/components/granite/form/fieldset"
                                title="Validation Message">
                                <items jcr:primaryType="nt:unstructured">
                                    <mandatoryMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="No Value"
                                        name="./mandatoryMessage"/>
                                    <validateExpMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Incorrect Value"
                                        name="./validateExpMessage"/>
                                </items>
                            </validationMessage>
                            <markOptional
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                fieldLabel="Validate CAPTCHA"
                                name="./mandatory">
                                <items jcr:primaryType="nt:unstructured">
                                    <ootb
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/radio"
                                        checked="true"
                                        fieldDescription="When the option is selected, AEM Forms validates CAPTCHA on submission of an adaptive form."
                                        text="Validate CAPTCHA at form submission"
                                        value="true"/>
                                    <custom
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/radio"
                                        fieldDescription="Use a custom method or the AEM Forms ValidateCAPTCHA API to validate a CAPTCHA while filling the form. You can use rule editor or other routes to invoke your custom method or the ValidateCAPTCHA API. For details, see the product documentation."
                                        text="Validate CAPTCHA on a user action"
                                        value="false"/>
                                </items>
                            </markOptional>
                            <serviceType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="CAPTCHA Service"
                                name="./captchaService"
                                required="{Boolean}true">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.renderSubDialog(&quot;captchaService&quot;)"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.renderSubDialog(&quot;captchaService&quot;)"/>
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    type="captchaService"/>
                            </serviceType>
                            <captchaConfiguration
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Configuration"
                                sling:resourceType="fd/af/authoring/components/granite/form/fieldset"
                                name="./captchaConfiguration"
                                title="Configuration">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    touchUIFormActionSet="captchaConfigurationSet"/>
                            </captchaConfiguration>
                            <visible
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <enabled
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <mandatoryMessage
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <validateExpMessage
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <placeholder
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <required
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <bindref
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <dorBindref
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <guideGlobalLazyField
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <fieldLayout
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <colspan
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <css
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                        </items>
                    </basic>
                    <dor
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <excludeFromDor
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"
                                checked="{Boolean}true"/>
                        </items>
                    </dor>
                    <patterns
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <help
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <accessibility
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                </items>
            </accordion>
        </items>
    </content>
</jcr:root>
