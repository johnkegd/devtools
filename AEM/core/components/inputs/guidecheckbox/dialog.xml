<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:mixinTypes="[granite:InternalArea]"
    jcr:primaryType="cq:Dialog"
    height="{Long}600"
    title="Edit checkBox"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <title
                jcr:primaryType="nt:unstructured"
                title="Title and text"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <name
                        jcr:primaryType="nt:unstructured"
                        path="/libs/fd/af/components/guidefield/dialog/items/title/items/name.infinity.json"
                        xtype="cqinclude"/>
                    <title
                        jcr:primaryType="nt:unstructured"
                        path="/libs/fd/af/components/guidefield/dialog/items/title/items/title.infinity.json"
                        xtype="cqinclude"/>
                    <colspan
                        jcr:primaryType="nt:unstructured"
                        path="/libs/fd/af/components/guidefield/dialog/items/title/items/colspan.infinity.json"
                        xtype="cqinclude"/>
                    <hideTitle
                        jcr:primaryType="nt:unstructured"
                        path="/libs/fd/af/components/guidefield/dialog/items/title/items/hideTitle.infinity.json"
                        xtype="cqinclude"/>
                    <guideGlobalLazyField
                        jcr:primaryType="nt:unstructured"
                        path="/libs/fd/af/components/guidefield/dialog/items/title/items/guideGlobalLazyField.infinity.json"
                        xtype="cqinclude"/>
                    <required
                        jcr:primaryType="nt:unstructured"
                        path="/libs/fd/af/components/guidefield/dialog/items/title/items/required.infinity.json"
                        xtype="cqinclude"/>
                    <mandatoryMessage
                        jcr:primaryType="nt:unstructured"
                        path="/libs/fd/af/components/guidefield/dialog/items/title/items/mandatoryMessage.infinity.json"
                        xtype="cqinclude"/>
                    <value
                        jcr:primaryType="nt:unstructured"
                        path="/libs/fd/af/components/guidetextbox/dialog/items/items/title/items/value.infinity.json"
                        xtype="cqinclude"/>
                    <options
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter values in {value}={text} format."
                        fieldLabel="Items"
                        name="./options"
                        typeHint="String[]"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="nt:unstructured"
                            allowBlank="{Boolean}false"
                            xtype="textfield"/>
                    </options>
                    <alignment
                        jcr:primaryType="cq:Widget"
                        defaultValue="guideFieldHorizontalAlignment"
                        fieldLabel="Items Alignment"
                        name="./alignment"
                        type="radio"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <horizontal
                                jcr:primaryType="nt:unstructured"
                                text="Horizontal"
                                value="guideFieldHorizontalAlignment"/>
                            <vertical
                                jcr:primaryType="nt:unstructured"
                                text="Vertical"
                                value="guideFieldVerticalAlignment"/>
                        </options>
                    </alignment>
                    <bindref
                        jcr:primaryType="nt:unstructured"
                        path="/libs/fd/af/components/guidefield/dialog/items/title/items/bindref.infinity.json"
                        xtype="cqinclude"/>
                    <validateExpMessage
                        jcr:primaryType="nt:unstructured"
                        path="/libs/fd/af/components/guidefield/dialog/items/title/items/validateExpMessage.infinity.json"
                        xtype="cqinclude"/>
                </items>
            </title>
            <script
                jcr:primaryType="nt:unstructured"
                path="/libs/fd/af/components/guidefield/dialog/items/script.infinity.json"
                xtype="cqinclude"/>
            <styling
                jcr:primaryType="nt:unstructured"
                path="/libs/fd/af/components/guidefield/dialog/items/styling.infinity.json"
                xtype="cqinclude"/>
            <help
                jcr:primaryType="nt:unstructured"
                path="/libs/fd/af/components/guidefield/dialog/items/help.infinity.json"
                xtype="cqinclude"/>
            <accessibility
                jcr:primaryType="nt:unstructured"
                title="Accessibility"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <assistPriority
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Screen reader precedence"
                        name="./assistPriority"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <customText
                                jcr:primaryType="nt:unstructured"
                                text="Custom text"
                                value="custom"/>
                            <caption
                                jcr:primaryType="nt:unstructured"
                                text="Title"
                                value="caption"/>
                        </options>
                    </assistPriority>
                    <customText
                        jcr:primaryType="nt:unstructured"
                        anchor="92.6%"
                        fieldLabel="Custom text"
                        name="./custom"
                        xtype="multifield"/>
                </items>
            </accessibility>
            <excludeFromDor
                jcr:primaryType="nt:unstructured"
                path="/libs/fd/af/components/guidefield/dialog/items/dorSettings.infinity.json"
                xtype="cqinclude"/>
        </items>
    </items>
    <listeners
        jcr:primaryType="nt:unstructured"
        path="/libs/fd/af/components/guidefield/dialog/listeners.infinity.json"
        xtype="cqinclude"/>
</jcr:root>
