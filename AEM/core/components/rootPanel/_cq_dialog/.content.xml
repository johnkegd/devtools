<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title=""
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[af.authoring.dialog]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <accordion
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldLabel="Name"
                                name="./name"
                                regexText="Element name should only contain characters or numbers"
                                required="{Boolean}true"
                                xtype="textfield">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    node-name-validation=""/>
                            </name>
                            <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                disabled="false"
                                fieldLabel="Panel Layout"
                                name="./layout/sling:resourceType"
                                renderReadOnly="false"
                                required="{Boolean}true">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    guideDataModel="basic"
                                    layoutType="fd/af/layouts/panel"
                                    type="layout"/>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.layoutOnChange('change');"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.layoutOnChange();"/>
                            </layout>
                            <nonNavigable
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./layout/nonNavigable"/>
                            <guideNavigatorTab
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./layout/guideNavigatorTab"/>
                            <enableLayoutOptimization
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./layout/enableLayoutOptimization"/>
                            <completionExpReq
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./completionExpReq"/>
                            <completionExpReq-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./completionExpReq@TypeHint"
                                value="Boolean"/>
                            <toolbarPosition
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                disabled="false"
                                fieldLabel="Toolbar Position"
                                name="./layout/toolbarPosition"
                                renderReadOnly="false">
                                <items jcr:primaryType="nt:unstructured">
                                    <v1
                                        jcr:primaryType="nt:unstructured"
                                        text="Bottom"
                                        value="Bottom"/>
                                    <v2
                                        jcr:primaryType="nt:unstructured"
                                        text="Top"
                                        value="Top"/>
                                </items>
                            </toolbarPosition>
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete"
                                disabled="false"
                                fieldLabel="Number of Columns"
                                name="./layout/columns"
                                renderReadOnly="false">
                                <options
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete/list"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <v0
                                        jcr:primaryType="nt:unstructured"
                                        text=""
                                        value=""/>
                                    <v1
                                        jcr:primaryType="nt:unstructured"
                                        text="1"
                                        value="1"/>
                                    <v2
                                        jcr:primaryType="nt:unstructured"
                                        text="2"
                                        value="2"/>
                                    <v3
                                        jcr:primaryType="nt:unstructured"
                                        text="3"
                                        value="3"/>
                                    <v4
                                        jcr:primaryType="nt:unstructured"
                                        text="4"
                                        value="4"/>
                                    <v5
                                        jcr:primaryType="nt:unstructured"
                                        text="5"
                                        value="5"/>
                                    <v6
                                        jcr:primaryType="nt:unstructured"
                                        text="6"
                                        value="6"/>
                                </items>
                            </columns>
                            <css
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="CSS Class"
                                name="./css"/>
                            <validateOnStepCompletion
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/panel/cq:dialog/content/items/accordion/items/basic/items/validateOnStepCompletion"
                                resourceType="granite/ui/components/coral/foundation/form/checkbox"/>
                            <validateOnStepCompletion-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/panel/cq:dialog/content/items/accordion/items/basic/items/validateOnStepCompletion-typehint"
                                resourceType="granite/ui/components/coral/foundation/form/hidden"/>
                            <visible
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/visible"
                                resourceType="granite/ui/components/coral/foundation/form/checkbox"/>
                            <visible-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/visible-typehint"
                                resourceType="granite/ui/components/coral/foundation/form/hidden"/>
                            <enabled
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/enabled"
                                resourceType="granite/ui/components/coral/foundation/form/checkbox"/>
                            <enabled-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/enabled-typehint"
                                resourceType="granite/ui/components/coral/foundation/form/hidden"/>
                        </items>
                        <parentConfig
                            jcr:primaryType="nt:unstructured"
                            active="{Boolean}true"/>
                    </basic>
                    <dor
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Document of Record"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <excludeFromDor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                checked="{Boolean}false"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/dor/items/excludeFromDor"
                                value="true"/>
                            <dorExcludeTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                checked="{Boolean}false"
                                name="./dorExcludeTitle"
                                text="Exclude title from Document of Record"
                                value="true"/>
                            <dorExcludeDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                checked="{Boolean}false"
                                name="./dorExcludeDescription"
                                text="Exclude description from Document of Record"
                                value="true"/>
                            <dorPagination
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Pagination"
                                sling:resourceType="fd/af/authoring/components/granite/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <breakBefore
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/granite/components/paginationpicker"
                                        emptyText="Following Previous"
                                        fieldDescription="Specifies where the panel is to be placed."
                                        fieldLabel="Place"
                                        name="./breakBeforeText"
                                        type="beforeBreakPaginationType"/>
                                    <breakAfter
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/granite/components/paginationpicker"
                                        emptyText="Continue Filling Parent"
                                        fieldDescription="Specifies which area to fill after the panel is placed."
                                        fieldLabel="After"
                                        name="./breakAfterText"
                                        type="afterBreakPaginationType"/>
                                    <overflow
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/granite/components/paginationpicker"
                                        emptyText="None"
                                        fieldDescription="Sets an overflow for a panel that spans pages."
                                        fieldLabel="Overflow"
                                        name="./overflowText"
                                        type="overflowPaginationType"/>
                                </items>
                            </dorPagination>
                        </items>
                        <granite:rendercondition
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="fd/af/dor/rendercondition/tab"/>
                    </dor>
                </items>
            </accordion>
        </items>
    </content>
</jcr:root>
