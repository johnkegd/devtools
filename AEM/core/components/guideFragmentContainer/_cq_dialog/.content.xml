<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title=""
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[af.authoring.dialog]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <accordion
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <useExistingAF
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <guideRef
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <redirect
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <prefill_service
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <parseGridLayoutForMenu
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <includeUnboundFields
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <excludeFieldsFromDoRIfHidden
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <clientLibRef
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Client library contains JavaScript functions and CSS needed for this fragment."
                                fieldLabel="Client library category"
                                name="./clientLibRef"/>
                            <layout
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    guideDataModel="basic"
                                    type="mobilelayout"/>
                            </layout>
                            <cssFileRef
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                            <themeRef
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    guideDataModel="basic"
                                    layoutType="fd/af/layouts/panel"
                                    type="theme"/>
                            </themeRef>
                            <fieldLayout
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    guideDataModel="basic"
                                    isAfField="false"
                                    layoutType="fd/af/layouts/field"
                                    type="layout"/>
                            </fieldLayout>
                        </items>
                    </basic>
                    <submitActions
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <autoSave
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <metadata
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                </items>
            </accordion>
        </items>
    </content>
</jcr:root>
