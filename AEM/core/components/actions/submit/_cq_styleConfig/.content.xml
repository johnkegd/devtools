<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="guideButton">
    <items jcr:primaryType="nt:unstructured">
        <button
            jcr:primaryType="nt:unstructured"
            jcr:title="Submit"
            cssSelector="button.submit:not(.af-icon-button)"
            id="af_submitbutton"
            jsonPostHandler="window.guidelib.touchlib.style.utils.buttonPostHandler('.guidebutton.submit', options)"
            longTitle="Submit Button"
            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"
            secondarySelectors="af_guideContainer:af_button">
            <items jcr:primaryType="nt:unstructured">
                <buttonText
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Button Text"
                    cssSelector="button.submit .iconButton-label"
                    id="af_submitbuttontext"
                    longTitle="Submit Button Text"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"
                    secondarySelectors="af_guideContainer:af_buttontext">
                    <states jcr:primaryType="nt:unstructured">
                        <focus
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Focus"
                            cssSelector="button.submit:not(.af-icon-button):focus .iconButton-label"/>
                        <active
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Down"
                            cssSelector="button.submit:not(.af-icon-button):active .iconButton-label"/>
                        <disabled
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Disabled"
                            cssSelector="\[data-disabled='true'] button.submit:not(.af-icon-button) .iconButton-label"/>
                        <hover
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Hover"
                            cssSelector="button.submit:not(.af-icon-button):hover .iconButton-label"/>
                    </states>
                </buttonText>
                <buttonIcon
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Button Icon"
                    cssSelector="button.submit .iconButton-icon"
                    id="af_submitbuttonicon"
                    longTitle="Submit Button Icon"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"
                    secondarySelectors="af_guideContainer:af_buttonicon">
                    <states jcr:primaryType="nt:unstructured">
                        <focus
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Focus"
                            cssSelector="button.submit:not(.af-icon-button):focus .iconButton-icon"/>
                        <active
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Down"
                            cssSelector="button.submit:not(.af-icon-button):active .iconButton-icon"/>
                        <disabled
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Disabled"
                            cssSelector="\[data-disabled='true'] button.submit:not(.af-icon-button) .iconButton-icon"/>
                        <hover
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Hover"
                            cssSelector="button.submit:not(.af-icon-button):hover .iconButton-icon"/>
                    </states>
                </buttonIcon>
            </items>
            <states jcr:primaryType="nt:unstructured">
                <focus
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Focus"
                    cssSelector="button.submit:not(.af-icon-button):focus"/>
                <active
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Down"
                    cssSelector="button.submit:not(.af-icon-button):active"/>
                <disabled
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Disabled"
                    cssSelector="\[data-disabled='true'] button.submit:not(.af-icon-button)"/>
                <hover
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Hover"
                    cssSelector="button.submit:not(.af-icon-button):hover"/>
            </states>
        </button>
    </items>
</jcr:root>
