    jcr:primaryType="nt:unstructured"
    jcr:title="Terms and Conditions"
    custom="Terms And Conditions"
    guideNodeClass="guideTermsAndConditions"
    showApprovalOption="{Boolean}true"
    showAsPopUp="{Boolean}false"
    showLink="{Boolean}false"
    tncCheckBoxContent="I agree to the terms &amp; conditions"
    tncTextContent="Text related to the terms and conditions come here">
    <items jcr:primaryType="nt:unstructured">
        <reviewDocument
            jcr:primaryType="nt:unstructured"
            guideNodeClass="guideTextBox"
            name="reviewDocument"/>
        <reviewStatus
            jcr:primaryType="nt:unstructured"
            guideNodeClass="guideCheckBox"
            name="reviewStatus"/>
    </items>
</jcr:root>
