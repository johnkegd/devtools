<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    componentId="af_guideContainer"
    label="Guide Container">
    <items jcr:primaryType="nt:unstructured">
        <afPage
            jcr:primaryType="nt:unstructured"
            jcr:title="Page"
            cssSelector="body"
            id="af_page"
            longTitle="Page"
            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"/>
        <form
            jcr:primaryType="nt:unstructured"
            jcr:title="Form"
            cssSelector=".guideContainer"
            id="af_form"
            longTitle="Form"
            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
            <items jcr:primaryType="nt:unstructured">
                <formTitle
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/guideformtitle/cq:styleConfig/items/formTitle"/>
            </items>
        </form>
        <field
            jcr:primaryType="nt:unstructured"
            jcr:title="Field"
            cssSelector=".guideFieldNode:not(.guideButton):not(.guideTextDraw):not(.guideChart)"
            id="af_field"
            longTitle="Field"
            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
            <states jcr:primaryType="nt:unstructured">
                <focus
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Focus"
                    cssSelector=".guideFieldNode.guideActiveField:not(.guideButton):not(.guideTextDraw):not(.guideChart)"/>
                <hover
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Hover"
                    cssSelector=".guideFieldNode:not(.guideButton):not(.guideTextDraw):not(.guideChart):hover"/>
                <disabled
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Disabled"
                    cssSelector="\[data-disabled='true'].guideFieldNode:not(.guideButton):not(.guideTextDraw):not(.guideChart)"/>
                <error
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Error"
                    cssSelector=".guideFieldNode.validation-failure:not(.guideButton):not(.guideTextDraw):not(.guideChart)"/>
                <success
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Success"
                    cssSelector=".guideFieldNode.validation-success:not(.guideButton):not(.guideTextDraw):not(.guideChart)"/>
                <mandatory
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Mandatory"
                    cssSelector="\[data-mandatory='true'].guideFieldNode:not(.guideButton):not(.guideTextDraw):not(.guideChart)"/>
            </states>
            <items jcr:primaryType="nt:unstructured">
                <fieldLabel
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/guidefield/cq:styleConfig/items/field/items/fieldLabel"/>
                <widgetAndText
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Widget"
                    cssSelector="input,select,textarea,.guideScribble img,.afTermsAndConditions"
                    id="af_widgetAndText"
                    jsonPostHandler="window.guidelib.touchlib.style.utils.widgetLabelPostHandler(' .guideFieldWidget:not(.guideFieldButtonWidget)', options)"
                    longTitle="Field Widget"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
                    <items jcr:primaryType="nt:unstructured">
                        <textBoxWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidetextbox/cq:styleConfig/items/field/items/widgetAndText"/>
                        <textAreaWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidetextbox/cq:styleConfig/items/field/items/textAreaWidgetAndText"/>
                        <richTextWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidetextbox/cq:styleConfig/items/field/items/richTextWidgetAndText"/>
                        <numericBoxWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidenumericbox/cq:styleConfig/items/field/items/widgetAndText"/>
                        <numericStepperWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidenumericstepper/cq:styleConfig/items/field/items/widgetAndText"/>
                        <passwordBoxWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidepasswordbox/cq:styleConfig/items/field/items/widgetAndText"/>
                        <emailWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guideemail/cq:styleConfig/items/field/items/widgetAndText"/>
                        <telephoneWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidetelephone/cq:styleConfig/items/field/items/widgetAndText"/>
                        <dropDownListWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidedropdownlist/cq:styleConfig/items/field/items/widgetAndText"/>
                        <datePickerWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidedatepicker/cq:styleConfig/items/field/items/widgetAndText"/>
                        <dateInputWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidedateinput/cq:styleConfig/items/field/items/widgetAndText"/>
                        <checkBoxGroupWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidecheckbox/cq:styleConfig/items/field/items/widgetAndText"/>
                        <radioButtonGroupWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guideradiobutton/cq:styleConfig/items/field/items/widgetAndText"/>
                        <imageChoiceGroupWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guideimagechoice/cq:styleConfig/items/field/items/widgetAndText"/>
                        <switchWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guideswitch/cq:styleConfig/items/field/items/widgetAndText"/>
                        <scribbleWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidescribble/cq:styleConfig/items/field/items/widgetAndText"/>
                        <fileUploadButton
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidefileupload/cq:styleConfig/items/field/items/widgetAndText"/>
                        <tncWidgetAndText
                            jcr:primaryType="nt:unstructured"
                            target="/mnt/override/libs/fd/af/components/guidetermsandconditions/cq:styleConfig/items/field/items/widgetAndText"/>
                    </items>
                    <states jcr:primaryType="nt:unstructured">
                        <focus
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Focus"
                            cssSelector="input:focus,select:focus,textarea:focus,.guideScribble img:focus"/>
                        <disabled
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Disabled"
                            cssSelector="input:disabled,select:disabled,textarea:disabled,.guideScribble img:disabled, [data-disabled='true'] input, [data-disabled='true'] select, [data-disabled='true'] textarea, [data-disabled='true'] .guideScribble img"/>
                        <hover
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Hover"
                            cssSelector="input:hover,select:hover,textarea:hover,.guideScribble img:hover"/>
                        <error
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Error"
                            cssSelector=".validation-failure input,.validation-failure select,.validation-failure textarea,.validation-failure.guideScribble img"/>
                        <success
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Success"
                            cssSelector=".validation-success input,.validation-success select,.validation-success textarea,.validation-success.guideScribble img"/>
                        <mandatory
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Mandatory"
                            cssSelector="\[data-mandatory='true'] input,[data-mandatory='true'] select,[data-mandatory='true'] textarea,[data-mandatory='true'].guideScribble img"/>
                    </states>
                </widgetAndText>
                <fieldDescriptionShort
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/guidefield/cq:styleConfig/items/field/items/fieldDescriptionShort"/>
                <fieldDescriptionLong
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/guidefield/cq:styleConfig/items/field/items/fieldDescriptionLong"/>
                <errorMessage
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/guidefield/cq:styleConfig/items/field/items/errorMessage"/>
            </items>
        </field>
        <button
            jcr:primaryType="nt:unstructured"
            jcr:title="Button">
            <items jcr:primaryType="nt:unstructured">
                <allButton
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/guidebutton/cq:styleConfig/items/button"/>
                <save
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/fp/components/actions/saveGuideDraft/cq:styleConfig/items/button"/>
                <submit
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/actions/submit/cq:styleConfig/items/button"/>
                <reset
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/actions/reset/cq:styleConfig/items/button"/>
                <attachments
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/actions/fileattachmentlisting/cq:styleConfig/items/button"/>
                <next
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/actions/nextitemnav/cq:styleConfig/items/button"/>
                <previous
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/actions/previtemnav/cq:styleConfig/items/button"/>
            </items>
        </button>
        <panel
            jcr:primaryType="nt:unstructured"
            jcr:title="Panel">
            <items jcr:primaryType="nt:unstructured">
                <responsivePanel
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/panel/cq:styleConfig/items/responsivePanel"/>
                <accordionPanel
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/panel/cq:styleConfig/items/accordionPanel"/>
                <tabsOnLeftPanel
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/panel/cq:styleConfig/items/tabsOnLeftPanel"/>
                <tabsOnTopPanel
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/panel/cq:styleConfig/items/tabsOnTopPanel"/>
                <wizardPanel
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/panel/cq:styleConfig/items/wizardPanel"/>
            </items>
        </panel>
        <panelHelp
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/af/components/panel/cq:styleConfig/items/panelHelp"/>
        <mobileMenu
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/af/components/guideContainer/cq:styleConfig/items/mobileMenu"/>
        <mobileTitleBar
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/af/components/guideContainer/cq:styleConfig/items/mobileTitleBar"/>
        <mobileScrollIndicator
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/af/components/guideContainer/cq:styleConfig/items/mobileScrollIndicator"/>
        <table
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/af/components/table/cq:styleConfig/items/table"/>
        <helpIcon
            jcr:primaryType="nt:unstructured"
            jcr:title="Help Icon"
            cssSelector=".guideHelpQuestionMark"
            id="af_helpicon"
            longTitle="Help Icon"
            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
            <items jcr:primaryType="nt:unstructured">
                <panelHelpIcon
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/panel/cq:styleConfig/items/panelHelpIcon"/>
                <fieldHelpIcon
                    jcr:primaryType="nt:unstructured"
                    target="/mnt/override/libs/fd/af/components/guidefield/cq:styleConfig/items/field/items/fieldHelpIcon"/>
            </items>
            <states jcr:primaryType="nt:unstructured">
                <hover
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Hover"
                    cssSelector=".guideHelpQuestionMark:hover"/>
            </states>
        </helpIcon>
        <toolbar
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/af/components/toolbar/cq:styleConfig/items/toolbar"/>
        <staticText
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/af/components/guidetextdraw/cq:styleConfig/items/staticText"/>
        <Image
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/af/components/guideimage/cq:styleConfig/items/Image"/>
        <chart
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/af/components/guidechart/cq:styleConfig/items/chart"/>
        <separator
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/af/components/guideseparator/cq:styleConfig/items/separator"/>
        <tooltip
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/af/components/guideContainer/cq:styleConfig/items/tooltip"/>
        <summary
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/afaddon/components/summary/cq:styleConfig/items/summary"/>
        <verify
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/afaddon/components/verify/cq:styleConfig/items/verify"/>
        <esign
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/afaddon/components/esign/cq:styleConfig/items/esign"/>
        <adobeSignBlock
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/afaddon/components/adobeSignBlock/cq:styleConfig/items/adobeSignBlock"/>
        <thankyoumessage
            jcr:primaryType="nt:unstructured"
            jcr:title="Thankyou Message"
            cssSelector=".tyMessage"
            id="af_thankyoumessage"
            longTitle="Thankyou Message"
            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"/>
        <documentFragment
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/ccm/web-channel/components/documentFragment/v1/documentFragment/cq:styleConfig/items/documentFragment"/>
        <documentFragmentGroup
            jcr:primaryType="nt:unstructured"
            target="/mnt/override/libs/fd/ccm/web-channel/components/documentFragmentGroup/v1/documentFragmentGroup/cq:styleConfig/items/documentFragmentGroup"/>
    </items>
</jcr:root>
