<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="fd/af/components/guideContainer"
    guideInline="{Boolean}true"
    guideNodeClass="guideContainerNode"
    name="guide1">
    <help
        jcr:primaryType="nt:unstructured"
        sling:resourceType="fd/af/components/HelpContainer">
        <helpPanel
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys"
            title="Panel Help"/>
    </help>
    <layout
        jcr:primaryType="nt:unstructured"
        sling:resourceType="fd/af/layouts/defaultGuideLayout"
        toolbarPosition="Bottom"/>
    <rootPanel
        jcr:primaryType="nt:unstructured"
        sling:resourceType="fd/af/components/rootPanel"
        guideNodeClass="rootPanelNode"
        name="rooPanel"
        panelSetType="Navigable">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="fd/af/layouts/panel/tabbedPanelLayout"
            enableLayoutOptimization="{Boolean}true"
            toolbarPosition="Bottom"/>
        <items jcr:primaryType="nt:unstructured">
            <panel1
                jcr:description="This is just a place to put your panel description"
                jcr:primaryType="nt:unstructured"
                jcr:title="Title1"
                sling:resourceType="fd/af/components/panel"
                guideNodeClass="guidePanel"
                name="GeneralInformation">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="fd/af/layouts/gridFluidLayout2"
                    toolbarPosition="Bottom"/>
                <items jcr:primaryType="nt:unstructured"/>
            </panel1>
        </items>
    </rootPanel>
    <toolbar
        jcr:primaryType="nt:unstructured"
        sling:resourceType="fd/af/components/toolbar"
        name="toolbar">
        <items jcr:primaryType="nt:unstructured">
            <submit
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/components/actions/submit"
                name="submit"
                submitFormat="dataXml"
                submitText="Submit"/>
            <reset
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/components/actions/reset"
                name="reset"
                resetText="Reset"/>
        </items>
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="fd/af/layouts/toolbar/defaultToolbarLayout"/>
    </toolbar>
</jcr:root>
