<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="guideContainer">
    <items jcr:primaryType="nt:unstructured">
        <mobileMenu
            jcr:primaryType="nt:unstructured"
            jcr:title="Mobile Menu"
            cssSelector=".tab-navigators.tab-navigators-mobile"
            id="af_mobilemenu"
            longTitle="Mobile Menu"
            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
            <items jcr:primaryType="nt:unstructured">
                <mobileMenuItem
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Menu Item"
                    cssSelector=".tab-navigators.tab-navigators-mobile > li > a"
                    id="af_mobilemenuitem"
                    longTitle="Mobile Menu Item"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
                    <states jcr:primaryType="nt:unstructured">
                        <active
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Active"
                            cssSelector=".tab-navigators.tab-navigators-mobile > li.active > a, .tab-navigators.tab-navigators-mobile > li.active.stepped > a"/>
                        <stepped
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Visited"
                            cssSelector=".tab-navigators.tab-navigators-mobile > li.stepped > a"/>
                        <disabled
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Disabled"
                            cssSelector=".tab-navigators.tab-navigators-mobile > li.wizard-tab-disabled > a, .tab-navigators.tab-navigators-mobile > li.wizard-tab-disabled > a:hover"/>
                        <hover
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Hover"
                            cssSelector=".tab-navigators.tab-navigators-mobile > li > a:hover"/>
                    </states>
                </mobileMenuItem>
                <mobileMenuButton
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Menu Button"
                    cssSelector=".guide-mobile-navigator .navigator-toggle"
                    id="af_mobilemenubutton"
                    longTitle="Mobile Menu Button"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
                    <states jcr:primaryType="nt:unstructured">
                        <hover
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Hover"
                            cssSelector=".guide-mobile-navigator .navigator-toggle:hover"/>
                    </states>
                </mobileMenuButton>
            </items>
        </mobileMenu>
        <mobileTitleBar
            jcr:primaryType="nt:unstructured"
            jcr:title="Mobile Title Bar"
            cssSelector=".guide-header-bar"
            id="af_mobiletitlebar"
            longTitle="Mobile Title Bar"
            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
            <items jcr:primaryType="nt:unstructured">
                <mobileTitleBarTitle
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Title"
                    cssSelector=".guide-header-bar .guide-breadcrumb"
                    id="af_mobiletitlebartitle"
                    longTitle="Mobile Title Bar Title"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"/>
                <mobileNextNav
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Next"
                    cssSelector=".guide-nav-next.guide-nav-toolbar-button"
                    id="af_mobiletitlebarnextnav"
                    longTitle="Mobile Title Bar Next"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"/>
                <mobilePrevNav
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Previous"
                    cssSelector=".guide-nav-prev.guide-nav-toolbar-button"
                    id="af_mobiletitlebarprevnav"
                    longTitle="Mobile Title Bar Previous"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"/>
            </items>
        </mobileTitleBar>
        <mobileScrollIndicator
            jcr:primaryType="nt:unstructured"
            jcr:title="Mobile Scroll Indicator"
            cssSelector=".mobileScrollIndicator"
            id="af_mobilescrollindicator"
            longTitle="Mobile Scroll Indicator"
            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
            <states jcr:primaryType="nt:unstructured">
                <hover
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Hover"
                    cssSelector=".navigator-toggle:hover"/>
            </states>
        </mobileScrollIndicator>
        <tooltip
            jcr:primaryType="nt:unstructured"
            jcr:title="Tooltip"
            cssSelector=".guideContainerWrapperNode .tooltip.bottom .tooltip-inner"
            id="af_tooltip"
            jsonPostHandler="window.guidelib.touchlib.style.utils.tooltipPostHandler(' .guideContainerWrapperNode .tooltip.bottom .tooltip-arrow',options)"
            longTitle="Tooltip"
            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"/>
    </items>
</jcr:root>
