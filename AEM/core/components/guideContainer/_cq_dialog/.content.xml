<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title=""
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[af.authoring.dialog]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <accordion
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <useExistingAF
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldDescription="Use an existing Adaptive Form."
                                name="./useExistingAF"
                                text="Use an existing Adaptive Form"
                                uncheckedValue="false"
                                value="true">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.useExistingAFOnChange();"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.useExistingAFOnChange();"/>
                            </useExistingAF>
                            <enableLayoutLayer
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldDescription="This form / document uses old lyouting capibilities. Check this to enable layouting using layout layer. Old layouting changes will be cleared on saving."
                                name="./enableLayoutLayer"
                                text="Enable Layout Layer"
                                uncheckedValue="false"
                                value="true">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.enableLayoutLayerOnLoad();"/>
                            </enableLayoutLayer>
                            <guideRef
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="fd/af/granite/components/formpicker"
                                fieldLabel="Adaptive Form Path"
                                filter="adaptiveForm"
                                name="./guideRef"
                                rootPath="/content/dam/formsanddocuments"/>
                            <prefill_service
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                disabled="false"
                                emptyText="Select"
                                fieldDescription="Select a prefill service to fetch form data when the form renders."
                                fieldLabel="Prefill Service"
                                name="./prefillService"
                                renderReadOnly="false">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    type="prefillServiceProvider"/>
                            </prefill_service>
                            <clientLibRef
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="This client library contains JavaScript Functions which can be used in expressions supported by Adaptive Forms."
                                fieldLabel="Client Library Category"
                                name="./clientLibRef"/>
                            <themeRef
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                disabled="false"
                                fieldLabel="Adaptive Form Theme"
                                name="./themeRef"
                                renderReadOnly="false">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    guideDataModel="basic"
                                    layoutType="fd/af/layouts/panel"
                                    type="theme"/>
                            </themeRef>
                            <cssFileRef
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="CSS File"
                                filter="nosystem"
                                name="./cssFileRef"
                                rootPath="/"/>
                            <mobileLayout
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Mobile Layout"
                                sling:resourceType="fd/af/authoring/components/granite/form/fieldset"
                                title="Mobile Layout">
                                <items jcr:primaryType="nt:unstructured">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        disabled="false"
                                        fieldLabel="Mobile Navigation"
                                        name="./layout/mobileLayout"
                                        renderReadOnly="false"
                                        required="true"
                                        value="fd/af/layouts/mobile/simple">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                            guideDataModel="basic"
                                            type="mobilelayout"/>
                                    </layout>
                                    <parseGridLayoutForMenu
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./layout/parseGridLayoutForMenu"
                                        text="Add navigable items of responsive layout to mobile menu"
                                        value="true"/>
                                </items>
                            </mobileLayout>
                            <fieldLayout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Layout for all the fields in the Adaptive Form."
                                fieldLabel="Field Layout"
                                name="./fieldLayout">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    guideDataModel="basic"
                                    isAfField="false"
                                    layoutType="fd/af/layouts/field"
                                    type="layout"/>
                            </fieldLayout>
                            <disableSwipeGesture
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldDescription="Disables the swipe gesture."
                                name="./disableSwipeGesture"
                                text="Disable Swipe Gesture"
                                uncheckedValue="false"
                                value="true"/>
                            <disableSwipeGesture-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./disableSwipeGesture@TypeHint"
                                value="Boolean"/>
                            <enableFocusOnFirstField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                checked="{Boolean}true"
                                fieldDescription="Select to keep the focus on the first field of the adaptive form or uncheck to keep the focus on the parent page or application."
                                name="./enableFocusOnFirstField"
                                text="Set Focus on Form"
                                uncheckedValue="false"
                                value="true"/>
                            <enableFocusOnFirstField-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./enableFocusOnFirstField@TypeHint"
                                value="Boolean"/>
                        </items>
                        <parentConfig
                            jcr:primaryType="nt:unstructured"
                            active="{Boolean}true"/>
                    </basic>
                    <autoSave
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Auto-save"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <autoSaveEnable
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enableAutoSave"
                                        text="Enable"
                                        value="true">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.showHideAutosaveConfig();"
                                            af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.showHideAutosaveConfig();"/>
                                    </autoSaveEnable>
                                    <autoSaveStartExpresion
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Auto save will be performed after this event."
                                        fieldLabel="Adaptive Form Event"
                                        name="./autoSaveStartExpression"/>
                                    <autoSaveStrategyType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Trigger"
                                        name="./autoSaveStrategyType">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.renderSubDialog(&quot;autoSaveAction&quot;)"
                                            af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.renderSubDialog(&quot;autoSaveAction&quot;)"/>
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                            guideDataModel="basic"
                                            type="autoSaveAction"/>
                                    </autoSaveStrategyType>
                                    <autoSaveStrategyActionSet
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Strategy Configuration"
                                        sling:resourceType="fd/af/authoring/components/granite/form/fieldset"
                                        name="./autoSaveStrategySet"
                                        title="Strategy Configuration">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            touchUIFormActionSet="autoSaveStrategySet"/>
                                    </autoSaveStrategyActionSet>
                                </items>
                            </columns>
                        </items>
                    </autoSave>
                    <submitActions
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Submission"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <enableAsyncSubmission
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enableAsyncSubmission"
                                        text="Use asynchronous submission"
                                        value="true">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.enableDisableAsyncSubmission();"
                                            af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.enableDisableAsyncSubmission();"/>
                                    </enableAsyncSubmission>
                                    <thankYouOption
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                        fieldLabel="On Submit"
                                        name="./thankYouOption">
                                        <items jcr:primaryType="nt:unstructured">
                                            <page
                                                jcr:primaryType="nt:unstructured"
                                                checked="{Boolean}true"
                                                text="Redirect to URL"
                                                value="page">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.thankYouOptionChange();"
                                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.thankYouOptionChange();"/>
                                            </page>
                                            <message
                                                jcr:primaryType="nt:unstructured"
                                                text="Show Message"
                                                value="message">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.thankYouOptionChange();"
                                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.thankYouOptionChange();"/>
                                            </message>
                                        </items>
                                    </thankYouOption>
                                    <redirect
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Redirect URL/Path"
                                        filter="nosystem"
                                        name="./redirect"/>
                                    <thankYouMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Message Content"
                                        name="./thankYouMessage"
                                        useFixedInlineToolbar="{Boolean}true"
                                        value="Thank you for submitting the form.">
                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                            <links jcr:primaryType="nt:unstructured">
                                                <linkDialogConfig
                                                    jcr:primaryType="nt:unstructured"
                                                    height="{Long}316">
                                                    <linkAttributes jcr:primaryType="cq:WidgetCollection"/>
                                                </linkDialogConfig>
                                            </links>
                                            <format
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <lists
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <justify
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <keys
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <paraformat
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <misctools
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <fullscreen
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                        </rtePlugins>
                                        <uiSettings jcr:primaryType="nt:unstructured">
                                            <cui jcr:primaryType="nt:unstructured">
                                                <inline
                                                    jcr:primaryType="nt:unstructured"
                                                    toolbar="[format#bold,format#italic,format#underline,fullscreen#start]">
                                                    <popovers jcr:primaryType="nt:unstructured">
                                                        <justify
                                                            jcr:primaryType="nt:unstructured"
                                                            items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                            ref="justify"/>
                                                        <lists
                                                            jcr:primaryType="nt:unstructured"
                                                            items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                            ref="lists"/>
                                                    </popovers>
                                                </inline>
                                                <fullscreen
                                                    jcr:primaryType="nt:unstructured"
                                                    toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,-,links#modifylink,links#unlink,links#anchor,tracklinks#modifylinktracking,-,justify#justifyleft,justify#justifycenter,justify#justifyright,-,image#imageProps,-,lists#unordered,lists#ordered,lists#outdent,lists#indent,-,misctools#sourceedit,fullscreen#finish]">
                                                    <popovers jcr:primaryType="nt:unstructured">
                                                        <styles
                                                            jcr:primaryType="nt:unstructured"
                                                            items="styles:getStyles:styles-pulldown"
                                                            ref="styles"/>
                                                        <paraformat
                                                            jcr:primaryType="nt:unstructured"
                                                            items="paraformat:getFormats:paraformat-pulldown"
                                                            ref="paraformat"/>
                                                    </popovers>
                                                </fullscreen>
                                            </cui>
                                        </uiSettings>
                                    </thankYouMessage>
                                    <enablePortalSubmit
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enablePortalSubmit"
                                        text="Store data in forms portal"
                                        value="true"/>
                                    <enableServerValidation
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enableServerValidation"
                                        text="Revalidate on server"
                                        value="true"/>
                                    <submitActionType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Submit Action"
                                        name="./actionType">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.renderSubDialog(&quot;submitAction&quot;);"
                                            submit-action-validation=""/>
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                            guideDataModel="basic"
                                            type="submitAction"/>
                                    </submitActionType>
                                    <actionSet
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Action Configuration"
                                        sling:resourceType="fd/af/authoring/components/granite/form/fieldset"
                                        name="./submitActionSet"
                                        title="Action Configuration">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            touchUIFormActionSet="submitActionSet"/>
                                    </actionSet>
                                    <xdpRef
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="xdpref"
                                        hidden="true"
                                        name="./xdpRef"/>
                                    <xsdRef
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="xsdref"
                                        hidden="true"
                                        name="./xsdRef"/>
                                </items>
                            </columns>
                        </items>
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            touchUIFormSubmitAction="touchUIFormSubmitAction"/>
                    </submitActions>
                    <signature
                        granite:class="signatureConfiguration"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Electronic Signature"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <enableAdobeSign
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./_useSignedPdf"
                                text="Enable Adobe Sign"
                                value="true"/>
                            <signConfigPath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Adobe Sign Cloud Service"
                                name="./signerInfo/signConfigPath"
                                required="{Boolean}true"
                                wrapperClass="signConfigPath">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    rootPath="/etc/cloudservices/echosign"
                                    type="esignCloudServiceConfiguration"/>
                            </signConfigPath>
                            <workflowType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Signers can sign"
                                name="./signerInfo/workflowType"
                                required="{Boolean}true"
                                wrapperClass="workflowType">
                                <items jcr:primaryType="nt:unstructured">
                                    <sequential
                                        jcr:primaryType="nt:unstructured"
                                        text="Sequentially – in the defined order"
                                        value="SEQUENTIAL"/>
                                    <parallel
                                        jcr:primaryType="nt:unstructured"
                                        text="Simultaneously – in any order"
                                        value="PARALLEL"/>
                                </items>
                            </workflowType>
                            <firstSignerFormFiller
                                granite:class="hiddenSignerField"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                defaultValue="{Boolean}false"
                                name="./signerInfo/firstSignerFormFiller"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <signerField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="fd/af/authoring/content/signField"
                                resouceType="granite/ui/components/coral/foundation/container"/>
                            <signDelete
                                granite:class="deleteField"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                hidden="true"
                                name="sign@Delete"/>
                            <firstSignerTable
                                granite:class="firstSignerTable"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Signers"
                                sling:resourceType="granite/ui/components/coral/foundation/table">
                                <columns jcr:primaryType="nt:unstructured">
                                    <order
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Signers"
                                        alignment="left"
                                        fixedwidth="{Boolean}true"
                                        order="{Boolean}true"/>
                                    <details
                                        jcr:primaryType="nt:unstructured"
                                        alignment="center"/>
                                    <edit
                                        jcr:primaryType="nt:unstructured"
                                        alignment="right"
                                        fixedwidth="{Boolean}true"/>
                                    <delete
                                        jcr:primaryType="nt:unstructured"
                                        alignment="right"
                                        fixedwidth="{Boolean}true"/>
                                </columns>
                                <items jcr:primaryType="nt:unstructured">
                                    <signerRow
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/authoring/components/granite/signerRow">
                                        <items jcr:primaryType="nt:unstructured">
                                            <dragHandle
                                                granite:class="signerDragButton"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/button"
                                                icon="dragHandle"
                                                iconSize="S"
                                                variant="quiet"/>
                                            <signerDetails
                                                granite:class="signerDetails"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <signerName
                                                        granite:class="signerName"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        name="signerName"
                                                        readonly="true"
                                                        value="Signer"
                                                        variant="quiet"/>
                                                    <signerConfig
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/include"
                                                        path="fd/af/authoring/content/signer"
                                                        resouceType="granite/ui/components/coral/foundation/container"/>
                                                </items>
                                            </signerDetails>
                                            <edit
                                                granite:class="signerEditButton"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/button"
                                                icon="edit"
                                                iconSize="S"
                                                variant="quiet"/>
                                            <delete
                                                granite:class="signerDeleteButton"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/button"
                                                icon="delete"
                                                iconSize="S"
                                                variant="quiet"/>
                                        </items>
                                    </signerRow>
                                </items>
                            </firstSignerTable>
                            <signerTable
                                granite:class="signerTable"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/table"
                                orderable="{Boolean}true"
                                skipEmptyRow="{Boolean}true">
                                <columns jcr:primaryType="nt:unstructured">
                                    <order
                                        jcr:primaryType="nt:unstructured"
                                        alignment="left"
                                        fixedwidth="{Boolean}true"
                                        order="{Boolean}true"/>
                                    <details
                                        jcr:primaryType="nt:unstructured"
                                        alignment="center"/>
                                    <edit
                                        jcr:primaryType="nt:unstructured"
                                        alignment="right"
                                        fixedwidth="{Boolean}true"/>
                                    <delete
                                        jcr:primaryType="nt:unstructured"
                                        alignment="right"
                                        fixedwidth="{Boolean}true"/>
                                </columns>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.loadSignerTable();"/>
                            </signerTable>
                            <addSigner
                                granite:class="signerAddButton"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/button"
                                text="Add Signer">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.loadAddSigner();"/>
                            </addSigner>
                        </items>
                    </signature>
                    <metadata
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Metadata"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/text"
                                text="Metadata For Drafts and Submissions"/>
                            <metadataProperties
                                granite:class="metadata-colon"
                                jcr:primaryType="nt:unstructured"
