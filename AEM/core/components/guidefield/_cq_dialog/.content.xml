<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title=""
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[af.authoring.dialog]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <accordion
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:mixinTypes="[granite:InternalArea]"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        active="{Boolean}true"
                        selected="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Name"
                                name="./name"
                                required="{Boolean}true">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    node-name-validation=""/>
                            </name>
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title"
                                name="./jcr:title"/>
                            <hideTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./hideTitle"
                                text="Hide title"
                                value="true"/>
                            <placeholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Placeholder Text"
                                name="./placeholderText"/>
                            <required
                                granite:class="requiredFieldProperty"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                checked="{Boolean}false"
                                fieldLabel="Required field"
                                labelledBy="Mandatory"
                                name="./mandatory"
                                text="Required field"
                                value="true">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.requiredFieldOnChange();"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.requiredFieldOnChange();"/>
                            </required>
                            <mandatoryMessage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldDescription="Error message shown when required field left empty."
                                fieldLabel="Required Field Message"
                                name="./mandatoryMessage"/>
                            <validateExpMessage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Displayed when validation script fails."
                                fieldLabel="Script Validation Message"
                                name="./validateExpMessage"/>
                            <bindref
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="fd/af/authoring/components/granite/form/bindrefselector"
                                emptyText="Data Model Object Reference"
                                fieldDescription="A valid bind reference can have  alphanumeric characters, parenthesis, dot, hyphen, at the rate, forward slash, asterisk, dollar and underscore."
                                fieldLabel="Bind Reference"
                                name="./bindRef">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    bindref-datamodel="fd.channelcore.dataObjectsTree.getDataModel"
                                    node-bindref-validation=""/>
                            </bindref>
                            <dorBindref
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="fd/af/authoring/components/granite/form/bindrefselector"
                                emptyText="AcroForm Object Reference"
                                fieldDescription="You can specify an AcroForm Data Binding to link an adaptive form field with a document of record field. When you fill any value in the linked field of an adaptive form, the value is also displayed in the linked field of the corresponding document of a record."
                                fieldLabel="AcroForm Data Binding"
                                name="./dorBindRef">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.showHideDoRBindRefField();"
                                    bindref-datamodel="fd.channelcore.dataObjectsTree.getDoRDataModel"/>
                            </dorBindref>
                            <guideGlobalLazyField
                                granite:class="global-lazy-field-margin"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldDescription="Enable to make the field value available even when the containing panel is not loaded initially or has unloaded."
                                label="Value accessible in lazy loading context"
                                name="./isGlobal"
                                text="Value accessible in lazy loading context"
                                value="true"/>
                            <visible
                                granite:class="hideFormObjectProperty"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./visible"
                                text="Hide Object"
                                uncheckedValue="true"
                                value="false">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.enableVisibleOnChange('visible');"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.enableVisibleOnLoad('visible');"/>
                            </visible>
                            <visible-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./visible@TypeHint"
                                value="Boolean"/>
                            <enabled
                                granite:class="disableFormObjectProperty"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./enabled"
                                text="Disable Object"
                                uncheckedValue="true"
                                value="false">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.enableVisibleOnChange('enabled');"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.enableVisibleOnLoad('enabled');"/>
                            </enabled>
                            <enabled-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./enabled@TypeHint"
                                value="Boolean"/>
                            <fieldLayout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                emptyText=""
                                fieldLabel="Field Layout"
                                name="./fieldLayout">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/components/commons/datasources/propertyprovider"
                                    guideDataModel="basic"
                                    isAfField="true"
                                    layoutType="fd/af/layouts/field"
                                    type="layout"/>
                            </fieldLayout>
                            <colspan
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete"
                                disabled="false"
                                emptyText=""
                                fieldDescription="Enter number of columns the field should span. Supported only for Responsive layout."
                                fieldLabel="Colspan"
                                name="./colspan"
                                renderReadOnly="false">
                                <options
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete/list"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <empty
                                        jcr:primaryType="nt:unstructured"
                                        text=""
                                        value=""/>
                                    <v1
                                        jcr:primaryType="nt:unstructured"
                                        text="1"
                                        value="1"/>
                                    <v2
                                        jcr:primaryType="nt:unstructured"
                                        text="2"
                                        value="2"/>
                                    <v3
                                        jcr:primaryType="nt:unstructured"
                                        text="3"
                                        value="3"/>
                                    <v4
                                        jcr:primaryType="nt:unstructured"
                                        text="4"
                                        value="4"/>
                                    <v5
                                        jcr:primaryType="nt:unstructured"
                                        text="5"
                                        value="5"/>
                                    <v6
                                        jcr:primaryType="nt:unstructured"
                                        text="6"
                                        value="6"/>
                                </items>
                            </colspan>
                            <css
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="CSS Class"
                                name="./css"/>
                            <toBeRemoved
                                jcr:primaryType="nt:unstructured"
                                sling:hideResource="{Boolean}true"/>
                        </items>
                        <parentConfig
                            jcr:primaryType="nt:unstructured"
                            active="{Boolean}true"/>
                    </basic>
                    <dor
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Document of Record"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <excludeFromDor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                checked="{Boolean}false"
                                name="./dorExclusion"
                                text="Exclude from Document of Record"
                                value="true"/>
                            <dorColspan
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                disabled="false"
                                emptyText=""
                                fieldDescription="Enter number of columns the field should span in Document of Record. Supported only for Column layout in Document of Record."
                                fieldLabel="Colspan for Document of Record"
                                name="./dorColspan"
                                renderReadOnly="false">
                                <items jcr:primaryType="nt:unstructured">
                                    <empty
                                        jcr:primaryType="nt:unstructured"
                                        text=""
                                        value=""/>
                                    <v1
                                        jcr:primaryType="nt:unstructured"
                                        text="1"
                                        value="1"/>
                                    <v2
                                        jcr:primaryType="nt:unstructured"
                                        text="2"
                                        value="2"/>
                                    <v3
                                        jcr:primaryType="nt:unstructured"
                                        text="3"
                                        value="3"/>
                                    <v4
                                        jcr:primaryType="nt:unstructured"
                                        text="4"
                                        value="4"/>
                                    <v5
                                        jcr:primaryType="nt:unstructured"
                                        text="5"
                                        value="5"/>
                                    <v6
                                        jcr:primaryType="nt:unstructured"
                                        text="6"
                                        value="6"/>
                                </items>
                            </dorColspan>
                        </items>
                        <granite:rendercondition
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="fd/af/dor/rendercondition/tab"/>
                    </dor>
                    <patterns
                        jcr:mixinTypes="[granite:InternalArea]"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Patterns"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </patterns>
                    <help
                        jcr:mixinTypes="[granite:InternalArea]"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Help Content"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <shortDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Short Description"
                                name="./shortDescription"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <links jcr:primaryType="nt:unstructured">
                                        <linkDialogConfig
                                            jcr:primaryType="nt:unstructured"
                                            height="{Long}316">
                                            <linkAttributes jcr:primaryType="cq:WidgetCollection"/>
                                        </linkDialogConfig>
                                    </links>
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <keys
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <misctools
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <fullscreen
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,fullscreen#start]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                    ref="justify"/>
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists"/>
                                            </popovers>
                                        </inline>
                                        <fullscreen
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,-,links#modifylink,links#unlink,links#anchor,tracklinks#modifylinktracking,-,justify#justifyleft,justify#justifycenter,justify#justifyright,-,image#imageProps,-,lists#unordered,lists#ordered,lists#outdent,lists#indent,-,misctools#sourceedit,fullscreen#finish]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <styles
                                                    jcr:primaryType="nt:unstructured"
                                                    items="styles:getStyles:styles-pulldown"
                                                    ref="styles"/>
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                            </popovers>
                                        </fullscreen>
                                    </cui>
                                </uiSettings>
                            </shortDescription>
                            <shortDescVisible
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./shortVisible"
                                text="Always show short description"
                                value="true"/>
                            <longDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Long Description"
                                name="./longDescription"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <links jcr:primaryType="nt:unstructured">
                                        <linkDialogConfig
                                            jcr:primaryType="nt:unstructured"
                                            height="{Long}316">
                                            <linkAttributes jcr:primaryType="cq:WidgetCollection"/>
                                        </linkDialogConfig>
                                    </links>
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <keys
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <misctools
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <fullscreen
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,fullscreen#start]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                    ref="justify"/>
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists"/>
                                            </popovers>
                                        </inline>
                                        <fullscreen
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,-,links#modifylink,links#unlink,links#anchor,tracklinks#modifylinktracking,-,justify#justifyleft,justify#justifycenter,justify#justifyright,-,image#imageProps,-,lists#unordered,lists#ordered,lists#outdent,lists#indent,-,misctools#sourceedit,fullscreen#finish]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <styles
                                                    jcr:primaryType="nt:unstructured"
                                                    items="styles:getStyles:styles-pulldown"
                                                    ref="styles"/>
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                            </popovers>
                                        </fullscreen>
                                    </cui>
                                </uiSettings>
                            </longDescription>
                        </items>
                    </help>
                    <accessibility
                        jcr:mixinTypes="[granite:InternalArea]"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Accessibility"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <assistPriority
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                emptyText="Select"
                                fieldLabel="Screen Reader Precedence"
                                name="./assistPriority"
                                value="custom">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    af.listeners.change="guidelib.touchlib.editLayer.dialogUtils.showHideCustomText()"
                                    af.listeners.onload="guidelib.touchlib.editLayer.dialogUtils.showHideCustomText()"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <empty
                                        jcr:primaryType="nt:unstructured"
                                        text=""
                                        value=""/>
                                    <customText
                                        jcr:primaryType="nt:unstructured"
                                        text="Custom Text"
                                        value="custom"/>
                                    <shortDescription
                                        jcr:primaryType="nt:unstructured"
                                        text="Short Description"
                                        value="shortDescription"/>
                                    <caption
                                        jcr:primaryType="nt:unstructured"
                                        text="Title"
                                        value="label"/>
                                    <name
                                        jcr:primaryType="nt:unstructured"
                                        text="Name"
                                        value="name"/>
                                    <none
                                        jcr:primaryType="nt:unstructured"
                                        text="None"
                                        value="none"/>
                                </items>
                            </assistPriority>
                            <customText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldLabel="Custom Text"
                                name="./custom"/>
                        </items>
                    </accessibility>
                </items>
            </accordion>
        </items>
    </content>
</jcr:root>
