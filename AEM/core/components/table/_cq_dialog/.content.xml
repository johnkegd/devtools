<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title=""
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[af.authoring.dialog]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <accordion
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:mixinTypes="[granite:InternalArea]"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/name"/>
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/title"/>
                            <description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Description"
                                name="./jcr:description"/>
                            <bindref
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/bindref"/>
                            <visible
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/visible"
                                resourceType="granite/ui/components/coral/foundation/form/checkbox"/>
                            <visible-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/visible-typehint"
                                resourceType="granite/ui/components/coral/foundation/form/hidden"/>
                            <enabled
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/enabled"
                                resourceType="granite/ui/components/coral/foundation/form/checkbox"/>
                            <enabled-typehint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="fd/af/components/guidefield/cq:dialog/content/items/accordion/items/basic/items/enabled-typehint"
                                resourceType="granite/ui/components/coral/foundation/form/hidden"/>
                            <enable-Sorting
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./sortingEnabled"
                                text="Enable Sorting"
                                uncheckedValue="false"
                                value="true"/>
                            <colspan
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete"
                                disabled="false"
                                emptyText=""
                                fieldDescription="Enter number of columns the field should span. Supported only for Responsive layout."
                                fieldLabel="Colspan"
                                name="./colspan"
                                renderReadOnly="false">
                                <options
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete/list"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <empty
                                        jcr:primaryType="nt:unstructured"
                                        text=""
                                        value=""/>
                                    <v1
                                        jcr:primaryType="nt:unstructured"
                                        text="1"
                                        value="1"/>
                                    <v2
                                        jcr:primaryType="nt:unstructured"
                                        text="2"
                                        value="2"/>
                                    <v3
                                        jcr:primaryType="nt:unstructured"
                                        text="3"
                                        value="3"/>
                                    <v4
                                        jcr:primaryType="nt:unstructured"
                                        text="4"
                                        value="4"/>
                                    <v5
                                        jcr:primaryType="nt:unstructured"
                                        text="5"
                                        value="5"/>
                                    <v6
                                        jcr:primaryType="nt:unstructured"
                                        text="6"
                                        value="6"/>
                                </items>
                            </colspan>
                            <css
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="CSS Class"
                                name="./css"/>
                            <columnWidth
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter a comma-separated list of values specifying proportionate width for table columns. Example: 1,2,3 as the column width for a three-column table makes the column width as 1/6, 2/6, and 3/6 among the first, second, and third columns, respectively."
                                fieldLabel="Column Width"
                                name="./columnWidth"/>
                            <mobileLayout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Mobile Layout"
                                name="./layout/mobileLayout">
                                <items jcr:primaryType="nt:unstructured">
                                    <headersLeft
                                        jcr:primaryType="nt:unstructured"
                                        text="Headers on Left"
                                        value="headersLeft"/>
                                    <collapsibleColumns
                                        jcr:primaryType="nt:unstructured"
                                        text="Collapsible Columns"
                                        value="collapsibleColumns"/>
                                </items>
                            </mobileLayout>
                        </items>
                        <parentConfig
                            jcr:primaryType="nt:unstructured"
                            active="{Boolean}true"/>
                    </basic>
                    <dor
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Document of Record"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <excludeFromDor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                checked="{Boolean}false"
                                path="fd/af/components/panel/cq:dialog/content/items/accordion/items/basic/items/dor/items/excludeFromDor"
                                value="true"/>
                            <dorExcludeTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                checked="{Boolean}false"
                                path="fd/af/components/panel/cq:dialog/content/items/accordion/items/basic/items/dor/items/dorExcludeTitle"
                                value="true"/>
                            <dorExcludeDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                checked="{Boolean}false"
                                path="fd/af/components/panel/cq:dialog/content/items/accordion/items/basic/items/dor/items/dorExcludeDescription"
                                value="true"/>
                            <dorPagination
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Pagination"
                                sling:resourceType="fd/af/authoring/components/granite/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <breakBefore
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/granite/components/paginationpicker"
                                        emptyText="Following Previous"
                                        fieldDescription="Specifies where the panel is to be placed."
                                        fieldLabel="Place"
                                        name="./breakBeforeText"
                                        type="beforeBreakPaginationType"/>
                                    <breakAfter
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/granite/components/paginationpicker"
                                        emptyText="Continue Filling Parent"
                                        fieldDescription="Specifies which area to fill after the panel is placed."
                                        fieldLabel="After"
                                        name="./breakAfterText"
                                        type="afterBreakPaginationType"/>
                                    <overflow
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/granite/components/paginationpicker"
                                        emptyText="None"
                                        fieldDescription="Sets an overflow for a panel that spans pages."
                                        fieldLabel="Overflow"
                                        name="./overflowText"
                                        type="overflowPaginationType"/>
                                </items>
                            </dorPagination>
                        </items>
                        <granite:rendercondition
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="fd/af/dor/rendercondition/tab"/>
                    </dor>
                    <help
                        jcr:mixinTypes="[granite:InternalArea]"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Help Content"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <longDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                disabled="{Boolean}false"
                                name="./longDescription"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <links jcr:primaryType="nt:unstructured">
                                        <linkDialogConfig
                                            jcr:primaryType="nt:unstructured"
                                            height="{Long}316">
                                            <linkAttributes jcr:primaryType="cq:WidgetCollection"/>
                                        </linkDialogConfig>
                                    </links>
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <keys
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <misctools
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <fullscreen
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,fullscreen#start]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                    ref="justify"/>
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists"/>
                                            </popovers>
                                        </inline>
                                        <fullscreen
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,-,links#modifylink,links#unlink,links#anchor,tracklinks#modifylinktracking,-,justify#justifyleft,justify#justifycenter,justify#justifyright,-,image#imageProps,-,lists#unordered,lists#ordered,lists#outdent,lists#indent,-,misctools#sourceedit,fullscreen#finish]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <styles
                                                    jcr:primaryType="nt:unstructured"
                                                    items="styles:getStyles:styles-pulldown"
                                                    ref="styles"/>
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                            </popovers>
                                        </fullscreen>
                                    </cui>
                                </uiSettings>
                            </longDescription>
                        </items>
                    </help>
                    <accessibility
                        jcr:mixinTypes="[granite:InternalArea]"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Accessibility"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <summaryText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldDescription="Provides a brief overview of how data has been organized into a table."
                                fieldLabel="Summary"
                                name="./tableSummaryText"/>
                        </items>
                    </accessibility>
                </items>
            </accordion>
        </items>
    </content>
</jcr:root>
