<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Table"
    guideNodeClass="guideTable">
    <layout
        jcr:primaryType="nt:unstructured"
        sling:resourceType="fd/af/layouts/tableLayout"
        columns="2"
        mobileLayout="headersLeft"
        rows="2"/>
    <items
        jcr:primaryType="nt:unstructured"
        sling:resourceType="fd/af/layouts/tableLayout">
        <HeaderRow
            jcr:primaryType="nt:unstructured"
            jcr:title="Header Row"
            sling:resourceType="fd/af/components/tableHeader"
            guideComponentType="true"
            guideNodeClass="guideTableRow"
            name="headerRow">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/layouts/table/headerLayout"
                nonNavigable="{Boolean}true"/>
            <items
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/layouts/table/headerLayout">
                <tableItem
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="fd/af/components/guidetextdraw"
                    _value=""
                    guideComponentType="true"
                    guideNodeClass="guideTextDraw"
                    name="headerItem"/>
                <tableItem1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="fd/af/components/guidetextdraw"
                    _value=""
                    guideComponentType="true"
                    guideNodeClass="guideTextDraw"
                    name="headerItem1"/>
            </items>
        </HeaderRow>
        <Row1
            jcr:primaryType="nt:unstructured"
            jcr:title="Row 1"
            sling:resourceType="fd/af/components/tableRow"
            guideComponentType="true"
            guideNodeClass="guideTableRow"
            name="Row1">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/layouts/table/rowLayout"
                nonNavigable="{Boolean}true"/>
            <items
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/layouts/table/rowLayout">
                <tableItem
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Text Box"
                    sling:resourceType="fd/af/components/guidetextbox"
                    guideComponentType="true"
                    guideNodeClass="guideTextBox"
                    hideTitle="{Boolean}true"
                    name="tableItem11"/>
                <tableItem1
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Text Box"
                    sling:resourceType="fd/af/components/guidetextbox"
                    guideComponentType="true"
                    guideNodeClass="guideTextBox"
                    hideTitle="{Boolean}true"
                    name="tableItem12"/>
            </items>
        </Row1>
        <Row2
            jcr:primaryType="nt:unstructured"
            jcr:title="Row 2"
            sling:resourceType="fd/af/components/tableRow"
            guideComponentType="true"
            guideNodeClass="guideTableRow"
            name="Row2">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/layouts/table/rowLayout"
                nonNavigable="{Boolean}true"/>
            <items
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/layouts/table/rowLayout">
                <tableItem
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Text Box"
                    sling:resourceType="fd/af/components/guidetextbox"
                    guideComponentType="true"
                    guideNodeClass="guideTextBox"
                    hideTitle="{Boolean}true"
                    name="tableItem21"/>
                <tableItem1
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Text Box"
                    sling:resourceType="fd/af/components/guidetextbox"
                    guideComponentType="true"
                    guideNodeClass="guideTextBox"
                    hideTitle="{Boolean}true"
                    name="tableItem22"/>
            </items>
        </Row2>
    </items>
</jcr:root>
