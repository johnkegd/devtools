<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="table">
    <items jcr:primaryType="nt:unstructured">
        <table
            jcr:primaryType="nt:unstructured"
            jcr:title="Table"
            cssSelector=".guideTableNode table"
            id="af_table"
            longTitle="Table"
            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
            <items jcr:primaryType="nt:unstructured">
                <tableToolbar
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Row Toolbar"
                    cssSelector=".guideTableNode table .guideTableRuntimeRightControls"
                    id="af_tabletoolbar"
                    longTitle="Table Toolbar"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
                    <items jcr:primaryType="nt:unstructured">
                        <tableRowAddButton
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Add Button"
                            cssSelector=".guideTableRuntimeAddControl"
                            id="af_tableaddbutton"
                            longTitle="Table Row Add Button"
                            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
                            <states jcr:primaryType="nt:unstructured">
                                <hover
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Hover"
                                    cssSelector=".guideTableRuntimeAddControl:hover"/>
                            </states>
                        </tableRowAddButton>
                        <tableRowRemoveButton
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Remove Button"
                            cssSelector=".guideTableRuntimeDeleteControl"
                            id="af_tableremovebutton"
                            longTitle="Table Row Remove Button"
                            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
                            <states jcr:primaryType="nt:unstructured">
                                <hover
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Hover"
                                    cssSelector=".guideTableRuntimeDeleteControl:hover"/>
                            </states>
                        </tableRowRemoveButton>
                        <tableRowEditButton
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Edit Button"
                            cssSelector=".guideTableRuntimeEditControl"
                            id="af_tableeditbutton"
                            longTitle="Table Row Edit Button"
                            propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
                            <states jcr:primaryType="nt:unstructured">
                                <hover
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Hover"
                                    cssSelector=".guideTableRuntimeEditControl:hover"/>
                            </states>
                        </tableRowEditButton>
                    </items>
                </tableToolbar>
                <SortButton
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Sort Button"
                    cssSelector=".sortingButton.random"
                    id="af_sortbutton"
                    longTitle="Sort Button"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common">
                    <states jcr:primaryType="nt:unstructured">
                        <ascending
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Ascending"
                            cssSelector=".sortingButton.asc"/>
                        <descending
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Descending"
                            cssSelector=".sortingButton.desc"/>
                    </states>
                </SortButton>
                <tableDescription
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Description"
                    cssSelector=".guideTableNode div.guidePanelDescription"
                    id="af_tabledescription"
                    longTitle="Table Description"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"/>
                <tableHeaderRow
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Header Row"
                    cssSelector=".guideTableNode table .guideTableRowNode.guideTableHeader:nth-child(even),.guideTableNode table .guideTableRowNode.guideTableHeader:nth-child(odd),.guideTableNode table .guideTableRowNode.guideTableHeader"
                    id="af_tableheaderrow"
                    longTitle="Table Header Row"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"/>
                <tableHeaderCell
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Header Cell"
                    cssSelector=".guideTableNode table .guideTableRowNode.guideTableHeader th"
                    id="af_tableheadercell"
                    longTitle="Table Header Cell"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"/>
                <tableRow
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Row"
                    cssSelector=".guideTableNode table tr"
                    id="af_tablerow"
                    longTitle="Table Row"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"/>
                <tableEvenRow
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Even Row"
                    cssSelector=".guideTableNode table tbody:nth-of-type(even) tr, .guideTableNode table tbody tr:nth-of-type(even)"
                    id="af_tableevenrow"
                    longTitle="Table Even Row"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"
                    secondarySelectors="af_guideContainer:af_tablerow"/>
                <tableOddRow
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Odd Row"
                    cssSelector=".guideTableNode table tbody:nth-of-type(odd) tr:nth-of-type(odd)"
                    id="af_tableoddrow"
                    longTitle="Table Odd Row"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"
                    secondarySelectors="af_guideContainer:af_tablerow"/>
                <tableCell
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Table Cell"
                    cssSelector=".guideTableNode table td"
                    id="af_tablecell"
                    longTitle="Table Cell"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"/>
                <tableEvenColumn
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Even Column"
                    cssSelector=".guideTableNode table td:nth-child(even)"
                    id="af_tableevencolumn"
                    longTitle="Table Even Column"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"
                    secondarySelectors="af_guideContainer:af_tablecell"/>
                <tableOddColumn
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Odd Column"
                    cssSelector=".guideTableNode table td:nth-child(odd)"
                    id="af_tableoddcolumn"
                    longTitle="Table Odd Column"
                    propertySheet="/mnt/overlay/fd/af/components/stylePropertySheet/common"
                    secondarySelectors="af_guideContainer:af_tablecell"/>
            </items>
        </table>
    </items>
</jcr:root>
